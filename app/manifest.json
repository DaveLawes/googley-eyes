{
  "name": "Googley Eyes",
  "version": "1.1",
  "description": "Web usage app",
  "manifest_version": 2,
  "browser_action": {
  },
  "content_security_policy": "script-src 'self' ''",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "32": "images/eyes32.png"
    }
  }
}
