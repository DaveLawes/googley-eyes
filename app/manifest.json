{
  "name": "Googley Eyes",
  "version": "1.1",
  "description": "Web usage app",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "32": "images/eyes32.png",
      "64": "images/eyes64.png",
      "128": "images/eyes128.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "chrome_url_overrides" : {
  "newtab": "newTab.html"
},
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
